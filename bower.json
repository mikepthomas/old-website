{
  "name": "mikepthomas.github.io",
  "description": "",
  "main": "gulpfile.js",
  "authors": [
    "Mike Thomas"
  ],
  "license": "BSD-2-Clause",
  "keywords": [
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "homepage": "https://github.com/mikepthomas/mikepthomas.github.io",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "1.3.3",
    "bootstrap": "bootstrap-sass#3.3.7",
    "handlebars": "4.0.8",
    "jquery": "3.2.0",
    "jspdf": "1.3.4",
    "moment": "2.18.1",
    "requirejs": "2.3.3"
  },
  "install": {
    "path": {
      "js": "libs",
      "map": "libs",
      "otf": "fonts/{key}",
      "eot": "fonts/{key}",
      "svg": "fonts/{key}",
      "ttf": "fonts/{key}",
      "woff": "fonts/{key}",
      "woff2": "fonts/{key}"
    },
    "sources": {
      "backbone": [
        "bower_components/backbone/backbone-min.js",
        "bower_components/backbone/backbone-min.map",
        "bower_components/backbone/backbone.js"
      ],
      "bootstrap": [
        "bower_components/bootstrap/assets/fonts/bootstrap/glyphicons-halflings-regular.eot",
        "bower_components/bootstrap/assets/fonts/bootstrap/glyphicons-halflings-regular.svg",
        "bower_components/bootstrap/assets/fonts/bootstrap/glyphicons-halflings-regular.ttf",
        "bower_components/bootstrap/assets/fonts/bootstrap/glyphicons-halflings-regular.woff",
        "bower_components/bootstrap/assets/fonts/bootstrap/glyphicons-halflings-regular.woff2",
        "bower_components/bootstrap/assets/javascripts/bootstrap.js",
        "bower_components/bootstrap/assets/javascripts/bootstrap.min.js"
      ],
      "handlebars": [
        "bower_components/handlebars/handlebars.js",
        "bower_components/handlebars/handlebars.min.js"
      ],
      "jquery": [
        "bower_components/jquery/dist/jquery.js",
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/jquery/dist/jquery.min.map"
      ],
      "jspdf": [
        "bower_components/jspdf/dist/jspdf.debug.js",
        "bower_components/jspdf/dist/jspdf.min.js"
      ],
      "moment": [
        "bower_components/moment/min/moment.min.js",
        "bower_components/moment/moment.js"
      ],
      "requirejs": [
        "bower_components/requirejs/require.js"
      ],
      "underscore": [
        "bower_components/underscore/underscore-min.js",
        "bower_components/underscore/underscore-min.map",
        "bower_components/underscore/underscore.js"
      ]
    }
  }
}
